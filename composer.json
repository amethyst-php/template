{
    "name": "amethyst/template",
    "keywords": ["laravel", "amethyst", "amethyst-template", "template"],
    "license": "MIT",
    "require": {
        "php": ">=7.2",
        "amethyst/core": "0.2.*",
        "amethyst/data-builder": "0.2.*"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "orchestra/testbench": "*",
        "friendsofphp/php-cs-fixer": "^2.8.1",
        "spatie/pdf-to-text": "^1.1"
    },
    "autoload": {
        "psr-4" : {
            "Amethyst\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": { 
            "Amethyst\\Tests\\": "tests/" 
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Amethyst\\Providers\\TemplateServiceProvider"
            ]
        }
    }
}
