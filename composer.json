{
    "name": "railken/amethyst-template",
    "keywords": ["laravel", "amethyst", "amethyst-template", "template"],
    "license": "MIT",
    "require": {
        "php": ">=7.1",
        "railken/amethyst-common": "0.1.*",
        "railken/amethyst-data-builder": "0.1.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.1",
        "orchestra/testbench": "3.*",
        "friendsofphp/php-cs-fixer": "^2.8.1",
        "spatie/pdf-to-text": "^1.1"
    },
    "autoload": {
        "psr-4" : {
            "Railken\\Amethyst\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": { 
            "Railken\\Amethyst\\Tests\\": "tests/" 
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Railken\\Amethyst\\Providers\\TemplateServiceProvider"
            ]
        }
    }
}
